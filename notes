docker build -t slk-cpp-builder .
docker run --rm -it -v "&{PWD}:/usr/src/app" slk-cpp-builder

    in container:
        conan profile detect --force
        conan profile show default
        conan install . --output-folder=build --build=missing


        mkdir build
        cd build
        cmake ..                cmake -G "Ninja" ..
        make -j4                ninja

